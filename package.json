{
  "name": "compiler",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build && npm run build:prefetch",
    "build:prefetch": "esbuild scripts/prefetch.ts --bundle --platform=node --outfile=build/prefetch.js --format=esm --external:bcrypt --external:postgres --packages=external",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "prefetch": "node build/prefetch.js"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.19",
    "@node-saml/node-saml": "^5.1.0",
    "@react-router/node": "7.10.1",
    "@react-router/serve": "7.10.1",
    "@tailwindcss/typography": "^0.5.19",
    "bcrypt": "^6.0.0",
    "chart.js": "^4.5.1",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "isbot": "^5.1.31",
    "postgres": "^3.4.7",
    "react": "^19.2.3",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "react-router": "7.10.1",
    "remark-gfm": "^4.0.1",
    "tailwind-scrollbar-hide": "^4.0.0"
  },
  "devDependencies": {
    "@react-router/dev": "7.10.1",
    "@tailwindcss/vite": "^4.1.13",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^22",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "esbuild": "^0.25.5",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.21.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
